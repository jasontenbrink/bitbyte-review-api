        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "generate-password": "^1.4.2",
        "knex": "^0.19.3",
        "lodash.camelcase": "^4.3.0",
        "lodash.snakecase": "^4.1.1",
        "moment": "^2.29.2",
        "passport": "^0.4.0",
